{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "ES2016",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "store/*": ["store/*"]
    }
  },
  "include": ["src"]
}
